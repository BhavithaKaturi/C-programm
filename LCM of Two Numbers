#include <stdio.h>
int main() {
    int a,b,gcd,lcm;
    printf("Enter two numbers: ");
    scanf("%d %d",&a,&b);
    int tempA=a, tempB=b;
    while(tempB!=0){
        int t = tempB;
        tempB = tempA%tempB;
        tempA = t;
    }
    gcd = tempA;
    lcm = (a*b)/gcd;
    printf("LCM: %d\n", lcm);
    return 0;
}
