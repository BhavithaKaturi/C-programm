#include <stdio.h>
#include <math.h>
int main() {
    int n, temp, rem, sum=0, digits=0;
    printf("Enter number: ");
    scanf("%d",&n);
    temp=n;
    while(temp) { digits++; temp/=10; }
    temp=n;
    while(temp) {
        rem = temp%10;
        sum += pow(rem,digits);
        temp/=10;
    }
    if(sum==n) printf("Armstrong Number\n");
    else printf("Not Armstrong\n");
    return 0;
}
